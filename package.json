{
  "name": "emf-sleep-guardian",
  "version": "1.0.0",
  "description": "A health-focused app to reduce EMF exposure and improve sleep quality",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack serve --config webpack.config.js",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace EMFSleepGuardian.xcworkspace -scheme EMFSleepGuardian -configuration Release -destination generic/platform=iOS -archivePath EMFSleepGuardian.xcarchive archive",
    "build:web": "webpack --config webpack.config.js --mode production",
    "clean": "react-native clean-project-auto"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "react-native-push-notification": "^8.1.1",
    "nostr-tools": "^1.15.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "react-native-permissions": "^3.8.0",
    "react-native-background-task": "^0.2.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^13.4.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.7.0",
    "react-native-screens": "^3.22.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/stack": "^6.3.17"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.0",
    "@react-native/metro-config": "^0.72.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.19.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "keywords": [
    "emf",
    "sleep",
    "health",
    "nostr",
    "mobile",
    "wellness",
    "electromagnetic",
    "radiation",
    "sleep-quality"
  ],
  "author": "EMF Sleep Guardian Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thebitcoinstreetjournal/emf-sleep-guardian.git"
  },
  "homepage": "https://github.com/thebitcoinstreetjournal/emf-sleep-guardian#readme"
}
